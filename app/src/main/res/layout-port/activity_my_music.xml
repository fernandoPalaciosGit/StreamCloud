<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/activity_my_music"
    tools:context="es.cruzalosdedos.fernandopalacios.StreamSound.MyMusic">
    
    <!--fragmento cargado desde MyMusic.class-->

    <ImageButton
        android:id="@+id/load_my_music_fab"
        android:layout_width="@dimen/floating_action_button_size_normal"
        android:layout_height="@dimen/floating_action_button_size_normal"
        android:background="@drawable/ripple_act_welcome"
        android:scaleType="center"
        android:elevation="@dimen/floating_action_button_elevation"
        android:src="@android:drawable/ic_menu_edit"
        android:stateListAnimator="@anim/anim_load_my_music_fab"
        android:layout_marginBottom="@dimen/floating_action_button_margin_normal"
        android:layout_marginEnd="@dimen/floating_action_button_margin_normal"
        android:layout_alignParentBottom="true"
        android:layout_alignParentEnd="true" />


</RelativeLayout>
